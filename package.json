{
	"name": "voice-memo",
	"description": "A service consisting of a Javascript (React) frontend and Node backend for quickly and easily recording, storing, removing, and sharing short audio recordings made natively in the browser.",
	"author": {
		"name": "54ac",
		"email": "me@54ac.ovh",
		"url": "https://github.com/54ac"
	},
	"repository": "github:54ac/voice-memo",
	"version": "0.3.0",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"dev": "concurrently \"vite --open\" \"nodemon server\\server.js\"",
