		"@mui/icons-material": "^5.11.16",
		"@mui/material": "^5.13.0",
		"clipboard-copy": "^4.0.1",
		"cookie-parser": "^1.4.5",
		"express": "^4.17.1",
		"helmet": "^7.0.0",
		"lowdb": "^1.0.0",
		"moment": "^2.27.0",
		"multer": "^1.4.5-lts.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@vitejs/plugin-react": "^4.0.0",
		"@babel/core": "^7.18.13",
		"@babel/eslint-parser": "^7.18.9",
		"@babel/preset-react": "^7.18.6",
		"concurrently": "^8.0.1",
